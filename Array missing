#include <stdio.h>

int main() {
    int size, i, j, k, count = 0;
    printf("Enter array size: ");
    scanf("%d", &size);
    int arr[size];

    printf("Enter 10 array elements:\n");
    for(i = 0; i < 10; i++) {
        scanf("%d", &arr[i]);
    }

    for(i = 0; i < 10; i++) {
        for(j = i + 1; j < 10; j++) {
            if(arr[i] == arr[j]) {
                for(k = 0; k < 10; k++) {
                    if(arr[k] != arr[i] && arr[k] != arr[j]) {
                        arr[k] = (count % 2 == 0) ? arr[i] : arr[j];
                        count++;
                    }
                }
                break;
            }
        }
    }

    printf("Resultant array:\n");
    for(i = 0; i < 10; i++) {
        printf("%d ", arr[i]);
    }
    printf("\n");

    return 0;
}
